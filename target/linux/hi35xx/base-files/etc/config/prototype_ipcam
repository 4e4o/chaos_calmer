
config ipcam 'global'
    option vendor             'jvt'
    option model              's130h18v'
    option soc                'hi3518ev200'
    option totmem             '64M'
    option sysmem             '40M'
    option flash              '8M'
    option sensor             'ar0130'

config ipcam 'system'
    option sensor_config      '/etc/sensors/ar0130_720p_line.ini'
    option web_port           '8080'
    option web_static         'false'
    option isp_stack          '16384'
    option venc_stack         '16384'
    option web_stack          '16384'

config ipcam 'isp'
    option align_width        '64'
    option max_pool_cnt       '16'
    option blk_cnt            '4'

config ipcam 'gpio'
    option alarm_in           '-'
    option alarm_out          '-'
    option i2c_sda            '-'
    option i2c_scl            '-'
    option ircut_1            '64'
    option ircut_2            '65'
    option led_sensor         '47'
    option reset              '-'

config ipcam 'ircut'
    option enable             'true'
    option check_interval     '30'
    option impulse_delay      '250'

config ipcam 'i2c'
    option enable             'false'
    option sensor             'bmp180'

config ipcam 'record'
    option enable             'false'
    option path               '/sdcard/records/'
    option file_duration      '10'
    option width              '1280'
    option height             '720'
    option fps                '25'
    option bitrate            '1024'

config ipcam 'http_post'
    option enable             'false'
    option host               'ipcam.octonix.net'
    option url                '/~rewm/000000000000/%Y/%m/%d/%H.%M.jpg'
    option width              '640'
    option height             '360'
    option qfactor            '95'
    option interval           '60'
    option login              'your_login'
    option password           'your_pass'

config ipcam 'mp4'
    option enable             'true'
    option width              '1280'
    option height             '720'
    option fps                '25'
    option bitrate            '1024'

config ipcam 'jpeg'
    option enable             'true'
    option width              '1280'
    option height             '720'
    option qfactor            '80'

config ipcam 'mjpeg'
    option enable             'true'
    option width              '1280'
    option height             '720'
    option fps                '10'
    option bitrate            '1024'

config ipcam 'rtsp'
    option enable             'true'
    option width              '1280'
    option height             '720'
    option fps                '15'
    option bitrate            '1024'
    option udp                'true'
    option tcp                 'false'
